package edu.fju.exam;

public class Bubble {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		int tmp;
		int n[]  = {45,81,9,72,4,6};
		for (int i=0;i<n.length;i++){
			for (int j=0;j<n.length;j++){
				if (n[i]>n[j]){
					tmp=n[i];
					n[i]=n[j];
					n[j]=tmp;
				}
			}
			
		}
		for (int k=0;k<n.length;k++){
			System.out.print(n[k]+" ");
		}
	}

}
